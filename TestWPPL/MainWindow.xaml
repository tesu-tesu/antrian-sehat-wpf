<basic1:MyWindow x:Class="TestWPPL.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TestWPPL"
        xmlns:basic1="clr-namespace:Velacro.UIElements.Basic;assembly=Velacro"
        mc:Ignorable="d"
        ResizeMode="NoResize"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Frame Name="mainFrame" NavigationUIVisibility="Hidden" Margin="78,0,0,0"/>
        <Button Name="loginButton_btn" Content="Login" HorizontalAlignment="Left" Margin="3,10,0,0" VerticalAlignment="Top" Width="75" Click="loginButton_btn_Click"/>
        <Button Name="registerButton_btn" Content="Register" HorizontalAlignment="Left" Margin="3,35,0,0" VerticalAlignment="Top" Width="75" Click="registerButton_btn_Click"/>
        <Button x:Name="dashboardButton_btn" Content="Dashboard" HorizontalAlignment="Left" Margin="3,60,0,0" VerticalAlignment="Top" Width="75" Click="dashboardButton_btn_Click"/>
    </Grid>
</basic1:MyWindow>
